{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "yago",
  "version": "0.1.0",
  "identifier": "yago",
  "build": {
    "frontendDist": "../src-ui/dist",
    "devUrl": "http://localhost:30000",
    "beforeDevCommand": "if [ -d src-ui ]; then cd src-ui; else cd ../src-ui; fi && npm run dev",
    "beforeBuildCommand": "if [ -d src-ui ]; then cd src-ui; else cd ../src-ui; fi && npm install && npm run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "YAGO",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "libs/*"
    ]
  }
}